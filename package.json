{
  "name": "fellou-electron-browser",
  "version": "1.0.0",
  "type": "module",
  "description": "fellou browser",
  "keywords": [],
  "main": "main.js",
  "author": "fellou",
  "license": "MIT",
  "scripts": {
    "watch": "npx babel --watch tabs/index.jsx --out-file tabs/compiled.js",
    "start": "npx babel tabs/index.jsx --out-file tabs/compiled.js && electron-forge start",
